        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <strong> Add Feedback: </strong> 
                        </div>
                        <div class="card-body">
                             <form method="post" enctype="multipart/form-data">
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="textarea-input" class=" form-control-label" style="font-weight: bold; margin-top: 5px;"> Feedback Message :</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <textarea name="feedback_message" rows="3" placeholder="Feedback Message" class="form-control" required></textarea>
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="email-input" class=" form-control-label" style="font-weight: bold; margin-top: 5px;">User Rating :</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <select name="user_rating" class="form-control">
											    <option value="0" class="form-control"> 0 </option>
											    <option value="1" class="form-control"> 1 </option> 
											    <option value="2" class="form-control"> 2 </option> 
											    <option value="3" class="form-control"> 3 </option> 
											    <option value="4" class="form-control"> 4 </option> 
											    <option value="5" class="form-control"> 5 </option>
									    </select>
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="email-input" class=" form-control-label" style="font-weight: bold; margin-top: 5px;">User Name :</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <input type="text" class="form-control" placeholder="User Name" name="user_name">
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="date-input" class=" form-control-label" style="font-weight: bold; margin-top: 5px;">Feedback Date :</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <!-- <input type="date" class="form-control" placeholder="Feedback Date" name="feedback_date"> -->
                                            <select name="feedback_date" class="form-control">
                                                    <option id="demo" class="form-control"><script>
                                                        var d = new Date().toLocaleString();
                                                        document.getElementById("demo").innerHTML = d;
                                                    </script></option>
                                            </select>
                                    </div>
                                </div>
                        </div>
                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary btn-sm"> Submit </button>&nbsp;&nbsp;<a href="/homepage"> <button type="button" class="btn btn-danger btn-sm">Cancel</button></a>
                        </form>
